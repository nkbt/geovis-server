  "devDependencies": {
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "eslint": "3.3.1",
    "eslint-config-airbnb-base": "5.0.3",
    "eslint-plugin-import": "1.14.0",
    "istanbul": "0.4.5",
    "mkdirp": "0.5.1",
    "tap-xunit": "1.4.0",
    "tape": "4.6.0"
